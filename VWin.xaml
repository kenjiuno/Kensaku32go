<Window x:Class="Kensaku32go.VWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="FTS情報" Height="412" Width="581" WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <ComboBox Name="cb" Grid.Row="0" ItemsSource="{Binding}" DataContext="{x:Null}" SelectionChanged="cb_SelectionChanged" FontSize="14">
            
        </ComboBox>
        <RichTextBox Name="rtb" Grid.Row="1" ScrollViewer.VerticalScrollBarVisibility="Visible" FontSize="14">
        </RichTextBox>
    </Grid>
</Window>
